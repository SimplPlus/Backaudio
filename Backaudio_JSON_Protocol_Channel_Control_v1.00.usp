		    Source_cloudRadio_Fb = 0;
			Source_cloudStory_Fb = 0; 
			Source_cloudNews_Fb = 0; 
			Source_localMusic_Fb = 0; 
			Source_FM_Fb = 1; 
			Source_AUX_Fb = 0;
		}
		if(rCMD$ = "PlayLocalMusic"){
		
			Source_cloudMusic_Fb = 0;
		    Source_cloudRadio_Fb = 0;
			Source_cloudStory_Fb = 0; 
			Source_cloudNews_Fb = 0; 
			Source_localMusic_Fb = 1; 
			Source_FM_Fb = 0; 
			Source_AUX_Fb = 0;
		}
		if(rCMD$ = "PlayCloudNews"){
		
			Source_cloudMusic_Fb = 0;
		    Source_cloudRadio_Fb = 0;
			Source_cloudStory_Fb = 0; 
			Source_cloudNews_Fb = 1; 
			Source_localMusic_Fb = 0; 
