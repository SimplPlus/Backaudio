//************************************************************** Volume

PUSH Vol_Up
{
	INTEGER i;

	CMD$ = "SetVolume";
    
	for(i = iVol to 31){
	
		if(Vol_Up = 1 && iVol < 31){
			iVol = i;
			MakeString(arg$, "\x22volume\x22: %d", iVol);
	
			MKString();
			delay(30);
		}ELSE{
			Break;
		}
	}
	
}
PUSH Vol_Dn
{
	INTEGER i;

	CMD$ = "SetVolume";
    
	for(i = iVol to 0 Step -1){
	
		if(Vol_Dn = 1 && iVol > 0){
			iVol = i;
			MakeString(arg$, "\x22volume\x22: %d", iVol);
	
			MKString();
			delay(30);
		}ELSE{
			Break;
		}
	}
	
}
PUSH Mute
{
	INTEGER i;

	CMD$ = "SetMute";

	if(Mute_Fb = 1){
		arg$ = "\x22muteStat\x22: \x22normal\x22";
//		Mute_Fb = 0;
	}Else{
    	arg$ = "\x22muteStat\x22: \x22mute\x22";
//		Mute_Fb = 1;
	}
	MKString();
	
}
RELEASE Vol_Up, Vol_Dn, Mute
{
	arg$ = "";
	CMD$ = "GetVolume";

	MKString();

	delay(50);

	CMD$ = "GetMute";

	MKString();
}
CHANGE Volume
